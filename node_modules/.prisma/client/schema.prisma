generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SensorData {
  id        Int      @id @default(autoincrement())
  type      String
  value     Float
  timestamp DateTime @default(now())
}
